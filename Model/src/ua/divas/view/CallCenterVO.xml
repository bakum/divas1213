      <LABEL
        ResId="ua.divas.view.CallCenterVO_LABEL"/>
    </SchemaBasedProperties>
  </Properties>
  <ViewAccessor
    Name="CityesView1"
    ViewObjectName="ua.divas.view.CityesView"
    RowLevelBinds="true"/>
  <ViewAccessor
    Name="UsersView1"
    ViewObjectName="ua.divas.view.UsersView"
    RowLevelBinds="true"/>
  <ListBinding
    Name="LOV_CityId"
    ListVOName="CityesView1"
    ListRangeSize="-1"
    NullValueFlag="start"
    NullValueId="${adfBundle['oracle.javatools.resourcebundle.SystemBundle']['NULL_VALUE_RESID']}"
    MRUCount="0">
    <AttrArray Name="AttrNames">
      <Item Value="CityId"/>
    </AttrArray>
    <AttrArray Name="AttrExpressions"/>
    <AttrArray Name="ListAttrNames">
      <Item Value="Id"/>
    </AttrArray>
    <AttrArray Name="ListDisplayAttrNames">
      <Item Value="Fullname"/>
    </AttrArray>
    <DisplayCriteria/>
  </ListBinding>
  <ListBinding
    Name="LOV_UserId"
    ListVOName="UsersView1"
    ListRangeSize="-1"
    NullValueFlag="none"
    MRUCount="0">
    <AttrArray Name="AttrNames">
      <Item Value="UserId"/>
    </AttrArray>
    <AttrArray Name="AttrExpressions"/>
    <AttrArray Name="ListAttrNames">
      <Item Value="Id"/>
    </AttrArray>
    <AttrArray Name="ListDisplayAttrNames">
      <Item Value="Login"/>
    </AttrArray>
    <DisplayCriteria/>
  </ListBinding>
  <SQLQuery><![CDATA[SELECT Kontragents.CITY_ID, 
       Kontragents.DELETED, 
       Kontragents.FULLNAME, 
       Kontragents.ID, 
       Kontragents.INN, 
       Kontragents.IS_BUYER, 
       Kontragents.IS_GROUP, 
       Kontragents.IS_MEASURER, 
       Kontragents.IS_SUPPLIER, 
       Kontragents.KPP, 
       Kontragents.NAMEFULL, 
       Kontragents.OKPO, 
       Kontragents.PARENT_ID, 
       Kontragents.PHOTO, 
       Kontragents.PREDEFINED, 
       Kontragents.UR_FIZ, 
       Kontragents.USER_ID, 
       Kontragents.VERSION,
       (select 0 from dual) editable,
       (select regstring FROM orders_by_dat o where o.kontrag_id = Kontragents.ID and
        rownum = 1) last_order
FROM  KONTRAGENTS Kontragents
where Kontragents.IS_BUYER = 1 and Kontragents.IS_GROUP = 0
order by Kontragents.FULLNAME]]></SQLQuery>
  <EntityUsage
    Name="Kontragents"
    Entity="ua.divas.model.Kontragents"/>
  <ViewAttribute
    Name="CityId"
    PrecisionRule="true"
    EntityAttrName="CityId"
    EntityUsage="Kontragents"
    AliasName="CITY_ID"
    LOVName="LOV_CityId">
    <Properties>
      <SchemaBasedProperties>
        <CONTROLTYPE
          Value="choice"/>
        <AUTOSUBMIT
          Value="true"/>
      </SchemaBasedProperties>
    </Properties>
  </ViewAttribute>
  <ViewAttribute
    Name="Deleted"
    IsNotNull="true"
    PrecisionRule="true"
    EntityAttrName="Deleted"
    EntityUsage="Kontragents"
    AliasName="DELETED"
    IsQueriable="false"/>
  <ViewAttribute
