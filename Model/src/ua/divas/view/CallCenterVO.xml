    IsPersistent="false"
    PrecisionRule="true"
    Type="java.math.BigDecimal"
    ColumnType="NUMBER"
    AliasName="EDITABLE"
    Expression="EDITABLE"
    SQLType="NUMERIC"
    IsQueriable="false"/>
  <ViewAttribute
    Name="LastOrder"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Precision="71"
    Type="java.lang.String"
    ColumnType="VARCHAR2"
    AliasName="LAST_ORDER"
    Expression="LAST_ORDER"
    SQLType="VARCHAR">
    <DesignTime>
      <Attr Name="_DisplaySize" Value="71"/>
    </DesignTime>
    <Properties>
      <SchemaBasedProperties>
        <LABEL
          ResId="ua.divas.view.CallCenterVO.LastOrder_LABEL"/>
        <LABEL_PLURAL
          ResId="ua.divas.view.CallCenterVO.LastOrder_LABEL_PLURAL"/>
        <TOOLTIP
          ResId="ua.divas.view.CallCenterVO.LastOrder_TOOLTIP"/>
