    IsNotNull="true"
    PrecisionRule="true"
    EntityAttrName="IsBuyer"
    EntityUsage="Kontragents"
    AliasName="IS_BUYER"
    IsQueriable="false">
    <TransientExpression
      trustMode="untrusted"><![CDATA[1]]></TransientExpression>
  </ViewAttribute>
  <ViewAttribute
    Name="IsGroup"
    IsNotNull="true"
    PrecisionRule="true"
    EntityAttrName="IsGroup"
    EntityUsage="Kontragents"
    AliasName="IS_GROUP"
    IsQueriable="false">
    <TransientExpression
      trustMode="untrusted"><![CDATA[0]]></TransientExpression>
  </ViewAttribute>
  <ViewAttribute
    Name="IsMeasurer"
    IsNotNull="true"
    PrecisionRule="true"
    EntityAttrName="IsMeasurer"
    EntityUsage="Kontragents"
    AliasName="IS_MEASURER"
    IsQueriable="false"/>
  <ViewAttribute
    Name="IsSupplier"
    IsNotNull="true"
    PrecisionRule="true"
    EntityAttrName="IsSupplier"
    EntityUsage="Kontragents"
    AliasName="IS_SUPPLIER"
    IsQueriable="false"/>
  <ViewAttribute
    Name="Kpp"
    PrecisionRule="true"
    EntityAttrName="Kpp"
    EntityUsage="Kontragents"
    AliasName="KPP"
    IsQueriable="false"/>
  <ViewAttribute
    Name="Namefull"
    PrecisionRule="true"
    EntityAttrName="Namefull"
    EntityUsage="Kontragents"
    AliasName="NAMEFULL"
    IsQueriable="false"/>
  <ViewAttribute
    Name="Okpo"
    PrecisionRule="true"
    EntityAttrName="Okpo"
    EntityUsage="Kontragents"
    AliasName="OKPO"
    IsQueriable="false"/>
  <ViewAttribute
    Name="ParentId"
    PrecisionRule="true"
    EntityAttrName="ParentId"
    EntityUsage="Kontragents"
    AliasName="PARENT_ID"
    IsQueriable="false">
    <TransientExpression
      trustMode="untrusted"><![CDATA[viewObject.retrieveCustomersFirstParentId()]]></TransientExpression>
  </ViewAttribute>
  <ViewAttribute
    Name="Photo"
    IsQueriable="false"
    PrecisionRule="true"
    EntityAttrName="Photo"
    EntityUsage="Kontragents"
    AliasName="PHOTO"/>
  <ViewAttribute
    Name="Predefined"
    PrecisionRule="true"
    EntityAttrName="Predefined"
    EntityUsage="Kontragents"
    AliasName="PREDEFINED"
    IsQueriable="false"/>
  <ViewAttribute
    Name="UrFiz"
    IsNotNull="true"
    PrecisionRule="true"
    EntityAttrName="UrFiz"
    EntityUsage="Kontragents"
    AliasName="UR_FIZ"
    IsQueriable="false"/>
  <ViewAttribute
    Name="UserId"
    IsNotNull="true"
    PrecisionRule="true"
    EntityAttrName="UserId"
    EntityUsage="Kontragents"
    AliasName="USER_ID"
    LOVName="LOV_UserId">
    <TransientExpression
      trustMode="untrusted"><![CDATA[viewObject.getSessionUserId()]]></TransientExpression>
    <Properties>
      <SchemaBasedProperties>
        <CONTROLTYPE
          Value="choice"/>
        <AUTOSUBMIT
          Value="true"/>
      </SchemaBasedProperties>
    </Properties>
  </ViewAttribute>
  <ViewAttribute
    Name="Version"
    PrecisionRule="true"
    EntityAttrName="Version"
    EntityUsage="Kontragents"
