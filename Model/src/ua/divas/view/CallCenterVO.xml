    IsNotNull="true"
    PrecisionRule="true"
    EntityAttrName="Fullname"
    EntityUsage="Kontragents"
    AliasName="FULLNAME">
    <Properties>
      <SchemaBasedProperties>
        <AUTOSUBMIT
          Value="true"/>
      </SchemaBasedProperties>
    </Properties>
  </ViewAttribute>
  <ViewAttribute
    Name="Id"
    IsNotNull="true"
    PrecisionRule="true"
    EntityAttrName="Id"
    EntityUsage="Kontragents"
    AliasName="ID"
    IsQueriable="false"/>
